{
  "name": "nagayamaryoga.github.io",
  "version": "0.0.1",
  "description": "NagayamaRyoga's web site.",
  "main": "index.js",
  "scripts": {
    "dev": "nuxt",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "build:posts": "processmd \"./static/posts/**/*.md\" --outputDir \"./static/posts\" --summaryOutput \"./static/posts/summary.json\" --preview 120",
    "build:gh-pages": "npm run build:posts && cross-env DEPLOY_ENV=GH_PAGES nuxt generate",
    "deploy:gh-pages": "npm run build:gh-pages && gh-pages -b master -t -d dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:NagayamaRyoga/NagayamaRyoga.github.io.git"
  },
  "author": "NagayamaRyoga",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-brands-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/vue-fontawesome": "^0.1.5",
    "cross-env": "^5.2.0",
    "nuxt": "^2.4.5",
    "nuxt-fontawesome": "^0.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "processmd": "^4.4.2",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^7.1.0"
  }
}
